{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "types": ["jest"],
    "jsx": "react-native",
    "esModuleInterop": true,
    "lib": ["es2015", "es2017"],
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true
